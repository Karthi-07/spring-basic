<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

 <bean id="car" class="org.example.Car" autowire="constructor">
    <property name="speed" value="50"></property>
     <constructor-arg value="50"></constructor-arg>
     <constructor-arg ref="com"></constructor-arg>
 </bean>

 <bean id="com" class="org.example.Honda">
 </bean>

 <bean id="ff" class="org.example.Shift">
 </bean>
</beans>
