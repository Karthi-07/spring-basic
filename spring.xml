<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
 <bean id="car" class="org.example.Car">
    <property name="speed" value="50"></property>
     <property name="shift" ref="shift"></property>
     <constructor-arg value="50"></constructor-arg>
     <constructor-arg ref="shift"></constructor-arg>
 </bean>
 <bean id="shift" class="org.example.Shift">
     <constructor-arg value="44"></constructor-arg>
 </bean>
</beans>
